# type: TH
#: isoquery.1:5
#, no-wrap
msgid "ISOQUERY"
msgstr "ISOQUERY"

# type: TH
#: isoquery.1:5
#, no-wrap
msgid "January 2, 2008"
msgstr "2. Januar 2008"

